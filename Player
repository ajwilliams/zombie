public class player {
    private String name;
    private static int brains;
    private static int shots;
    player(String n,int b, int s) {
   	 name = n;
   	 brains = b;
   	 shots = s;
    }
    
    String getName() {return name;}
    void setName(String n) {name = n;}
    int static getBrains() {return brains;}
    void static setBrians(int b) {brains = b;}
    int static getShots() {return shots;}
    void static setShots(int s) {shots = s;}
}
