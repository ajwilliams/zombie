public class player {
    private String name;
    private int brains;
    private int shots;
    player(String n,int b, int s) {
   	 name = n;
   	 brains = b;
   	 shots = s;
    }
    
    String getName() {return name;}
    void setName(String n) {name= n;}
    int getBrains() {return brains;}
    void setBrians(int b) {brains = b;}
    int getshots() {return shots;}
    void setshots(int s) {shots = s;}
}
